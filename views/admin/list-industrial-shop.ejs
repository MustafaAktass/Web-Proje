<div class="col-lg-12 grid-margin stretch-card">
    <div class="card">
      <div class="card-body">
        <h4 class="card-title">İşletmeler</h4>
        <table class="table table-bordered">
          <thead>
            <tr>
              <th> # </th>
              <th> İşletme Adı </th>
              <th> Kategori </th>
              <th> Şehir </th>
              <th> Detay </th>
            </tr>
          </thead>
          <tbody>
            <% for( let index = 0; index < data.length; index++ ) { %>
              <tr>
                <td> <%= index+1 %> </td>
                <td> <%= data[index].IsletmeAdi %> </td>
                <td><%= data[index].Kategori %> </td>
                <td> <%= data[index].Sehir %> </td>
                <td> Detay </td>
              </tr>
            <% } %>
          </tbody>
        </table>
      </div>
    </div>
  </div>